{
  "presets": [
    [
      "@babel/preset-env",
    { 
      "useBuiltIns": "usage",
      "corejs": {
          "version": 2,
          "proposals": true
        } 
      }
  ],"@babel/preset-react"],
  "plugins": [
    "syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css" // `style: true` 会加载 less 文件
    }]
  ]
}
